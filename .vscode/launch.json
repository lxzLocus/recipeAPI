  "version": "0.2.0",
  "configurations": [
    {
      "name": "Docker Debug JavaScript",
      "type": "node",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "program": "${file}",
      "outFiles": ["app/**/*.js"]
    },
    {
      "name": "Docker Debug TypeScript",
      "type": "node",
      "request": "launch",
      "preLaunchTask": "Compile TypeScript",
      "program": "${file}",
      "cwd": "${workspaceFolder}",
      // "args": ["aaa"]
    },
    {
      "name": "Use Stdin",
      "type": "node",
      "request": "launch",
      "skipFiles": ["<node_internals>/**"],
      "program": "${file}",
      "outFiles": ["app/**/*.js"],
      "args": [""]
    },
  ]
}
